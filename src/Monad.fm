type Monad <M: Type -> Type> {
  new(
    bind: <A: Type, B: Type> (m: M(A)) (f: (x:A) M(B)) M(B),
    pure: <A: Type> (x: A) M(A),
  )
}

Monad.pure<M: Type -> Type>(m: Monad(M)): <A: Type> -> A -> M(A)
  open m
  m.pure

Monad.bind<M: Type -> Type>(m: Monad(M)): <A: Type, B: Type> -> M(A) -> (A -> M(B)) -> M(B)
  open m
  m.bind

